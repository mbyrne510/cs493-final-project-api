{{!-- syntax for Bootstrap navbar sourced from:
https://getbootstrap.com/docs/4.1/components/navbar/ --}}

<nav class="navbar navbar-expand-lg navbar-light bg-light">
  JWT Retrieval
</nav>
<h4>This page displays the JWT generated by the Google OAuth 2.0 API:</h4>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Token</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{jwtToken}}</td>
    </tr>
  </tbody>
  <thead>
    <tr>
      <th scope="col">User Id</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{jwtSub}}</td>
    </tr>
  </tbody>
</table>